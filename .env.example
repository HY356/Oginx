# 数据库配置 - 根据实际环境修改
# Database Configuration
DATABASE_URL=mysql+pymysql://username:password@localhost:3306/ollama_db

# 日志级别 - DEBUG, INFO, WARNING, ERROR
# Log Level
LOG_LEVEL=INFO

# 服务端口
# Server Port
SERVER_PORT=8083

# 配置文件路径
# Configuration File Path
CONFIG_FILE=config/app.yaml

# 资源监控服务配置
# Resource Monitor Service Configuration
RESOURCE_MONITOR_HOST=localhost
RESOURCE_MONITOR_PORT=8006
RESOURCE_MONITOR_CPU_PORT=8005
RESOURCE_MONITOR_GPU_PORT=8006
